{
  "name": "train-task",
  "version": "0.9.0",
  "description": "制图软件",
  "author": "Peng Chaoyang",
  "private": true,
  "dependencies": {
    "@antv/g2": "^3.1.2",
    "antd": "^3.6.1",
    "electron-packager": "^12.1.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "moment": "^2.22.2",
    "react": "^16.4.0",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "shortid": "^2.2.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "estart": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "packager": "electron-packager ./ train-task --platform=win32 --out ./outApp --app-version 1.0.0 --electron-version 2.0.2 --overwrite --icon=./public/favicon.ico",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win"
  },
  "main": "main.js",
  "homepage": ".",
  "DEV": true,
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1"
  },
  "build": {
    "extends": null,
    "productName": "train task",
    "compression": "normal",
    "electronVersion": "2.0.2",
    "directories": {
      "output": "./builder_output"
    },
    "files": [
      "*.js",
      "build",
      "node_modules"
    ],
    "win": {
      "target": "nsis",
      "icon": "./build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "extraFiles": [
      "dll"
    ]
  }
}
